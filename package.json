{
  "name": "karriere-kolleg",
  "type": "module",
  "version": "0.0.1",
  "scripts": {
    "dev": "astro dev --host",
    "build": "astro check && astro build",
    "build:cdx": "cp .env.cdx .env && npm run build",
    "preview": "astro preview --host",
    "astro": "astro",
    "format -c": "prettier --check .",
    "format": "prettier --write .",
    "format -c -s": "prettier --check src/**/*.astro",
    "format -s": "prettier --write src/**/*.astro",
    "lint": "eslint 'src/**/*.{ts,tsx,astro}'",
    "lint:fix": "eslint 'src/**/*.{ts,tsx,astro}' --fix",
    "check": "astro check"
  },
  "dependencies": {
    "@astrojs/check": "^0.9.4",
    "@astrojs/react": "^4.2.7",
    "@tailwindcss/vite": "^4.1.6",
    "@types/react": "^19.1.4",
    "@types/react-dom": "^19.1.5",
    "astro": "^5.7.13",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "tailwindcss": "^4.1.6",
    "typescript": "^5.8.3"
  },
  "devDependencies": {
    "@astrojs/ts-plugin": "^1.10.4",
    "@eslint/js": "^9.26.0",
    "@typescript-eslint/eslint-plugin": "^8.32.1",
    "@typescript-eslint/parser": "^8.32.1",
    "eslint": "^9.26.0",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-astro": "^1.3.1",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "prettier": "3.5.3",
    "prettier-plugin-astro": "0.14.1",
    "typescript-eslint": "^8.32.1"
  }
}
